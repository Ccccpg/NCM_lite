.touch-area {
  height: 40px;
}
.my-progress {
  width: var(--6a89fba6);
  margin-bottom: 10rpx;
}
.my-progress .movable-area {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: var(--2081ae80);
}
.my-progress .movable-area .movable-view {
  width: var(--2081ae80);
  height: var(--2081ae80);
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.5);
}
.my-progress .movable-area .sub-line {
  display: flex;
  width: var(--1929a5f2);
}
.my-progress .movable-area .sub-line .gone {
  width: var(--427fb95a);
  height: 5px;
  background-color: black;
}
.my-progress .movable-area .sub-line .remain {
  flex: 1;
  height: 5px;
  background-color: white;
}